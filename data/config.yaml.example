bot:
  token:
  description: Music bot
  playing status: music

# Where the bot will look for translations
locale_path: ./localization

# Settings used if server settings aren't specified
default server settings:
  prefix:
    - "!"
  moderator_role: Moderator
  locale: en_en
  threshold: 50

lavalink nodes:
  main:  # The preferred lavalink node for your music bot
    host: localhost
    port: 2333
    password: youshallnotpass
    region: eu
    name: default-node

  # Backup nodes for your music bot
  backup:
    - host: localhost
      port: 2333
      password: youshallnotpass
      region: eu
      name: backup-1